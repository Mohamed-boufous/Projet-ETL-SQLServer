      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8F8E2070-65E0-4352-A891-823CBFD62C7F}"
          SQLTask:SqlStatementSource="BEGIN TRY&#xA;    BEGIN TRANSACTION;&#xA;&#xA;    --------------------------------------------------&#xA;    -- 1. SUPPRESSION DES CLÉS ÉTRANGÈRES EXISTANTES&#xA;    -- (Nécessaire avant de faire un TRUNCATE)&#xA;    --------------------------------------------------&#xA;    IF OBJECT_ID('dbo.FK_Fact_Student', 'F') IS NOT NULL&#xA;        ALTER TABLE dbo.Fact_Performance DROP CONSTRAINT FK_Fact_Student;&#xA;&#xA;    IF OBJECT_ID('dbo.FK_Fact_Parent', 'F') IS NOT NULL&#xA;        ALTER TABLE dbo.Fact_Performance DROP CONSTRAINT FK_Fact_Parent;&#xA;&#xA;    IF OBJECT_ID('dbo.FK_Fact_Activities', 'F') IS NOT NULL&#xA;        ALTER TABLE dbo.Fact_Performance DROP CONSTRAINT FK_Fact_Activities;&#xA;&#xA;    --------------------------------------------------&#xA;    -- 2. GESTION DES TABLES (CRÉATION OU VIDAGE)&#xA;    --------------------------------------------------&#xA;&#xA;    -- === Table: Dim_Activities ===&#xA;    IF OBJECT_ID('dbo.Dim_Activities', 'U') IS NOT NULL&#xA;    BEGIN&#xA;        -- Si elle existe, on la vide&#xA;        TRUNCATE TABLE dbo.Dim_Activities;&#xA;    END&#xA;    ELSE&#xA;    BEGIN&#xA;        -- Si elle n'existe pas, on la crée&#xA;        CREATE TABLE [dbo].[Dim_Activities](&#xA;            [ActivitiesKey] [int] IDENTITY(1,1) NOT NULL,&#xA;            [Tutoring] [varchar](50) NULL,&#xA;            [Extracurricular] [varchar](50) NULL,&#xA;            [Sports] [varchar](50) NULL,&#xA;            [Music] [varchar](50) NULL,&#xA;            [Volunteering] [varchar](50) NULL,&#xA;            PRIMARY KEY CLUSTERED ([ActivitiesKey] ASC)&#xA;        );&#xA;    END&#xA;&#xA;    -- === Table: Dim_ParentalContext ===&#xA;    IF OBJECT_ID('dbo.Dim_ParentalContext', 'U') IS NOT NULL&#xA;    BEGIN&#xA;        TRUNCATE TABLE dbo.Dim_ParentalContext;&#xA;    END&#xA;    ELSE&#xA;    BEGIN&#xA;        CREATE TABLE [dbo].[Dim_ParentalContext](&#xA;            [ParentalContextKey] [int] IDENTITY(1,1) NOT NULL,&#xA;            [ParentalEducation] [varchar](50) NULL,&#xA;            [ParentalSupport] [varchar](50) NULL,&#xA;            PRIMARY KEY CLUSTERED ([ParentalContextKey] ASC)&#xA;        );&#xA;    END&#xA;&#xA;    -- === Table: Dim_Student ===&#xA;    IF OBJECT_ID('dbo.Dim_Student', 'U') IS NOT NULL&#xA;    BEGIN&#xA;        TRUNCATE TABLE dbo.Dim_Student;&#xA;    END&#xA;    ELSE&#xA;    BEGIN&#xA;        CREATE TABLE [dbo].[Dim_Student](&#xA;            [StudentID] [int] NOT NULL,&#xA;            [Age] [varchar](50) NULL,&#xA;            [Gender] [varchar](50) NULL,&#xA;            [Ethnicity] [varchar](50) NULL,&#xA;            PRIMARY KEY CLUSTERED ([StudentID] ASC)&#xA;        );&#xA;    END&#xA;&#xA;    -- === Table: Fact_Performance ===&#xA;    IF OBJECT_ID('dbo.Fact_Performance', 'U') IS NOT NULL&#xA;    BEGIN&#xA;        TRUNCATE TABLE dbo.Fact_Performance;&#xA;    END&#xA;    ELSE&#xA;    BEGIN&#xA;        CREATE TABLE [dbo].[Fact_Performance](&#xA;            [StudentID] [int] NOT NULL,&#xA;            [ParentalContextKey] [int] NOT NULL,&#xA;            [ActivitiesKey] [int] NOT NULL,&#xA;            [StudyTimeWeekly] [decimal](5, 2) NULL,&#xA;            [Absences] [int] NULL,&#xA;            [GPA] [decimal](5, 3) NULL,&#xA;            [GradeClass] [decimal](3, 1) NULL,&#xA;            CONSTRAINT [PK_Fact_Performance] PRIMARY KEY CLUSTERED &#xA;            (&#xA;                [StudentID] ASC,&#xA;                [ParentalContextKey] ASC,&#xA;                [ActivitiesKey] ASC&#xA;            )&#xA;        );&#xA;    END&#xA;&#xA;    --------------------------------------------------&#xA;    -- 3. RECRÉATION DES CONTRAINTES DE CLÉ ÉTRANGÈRE&#xA;    --------------------------------------------------&#xA;    ALTER TABLE [dbo].[Fact_Performance]  WITH CHECK ADD  CONSTRAINT [FK_Fact_Activities] FOREIGN KEY([ActivitiesKey])&#xA;    REFERENCES [dbo].[Dim_Activities] ([ActivitiesKey]);&#xA;&#xA;    ALTER TABLE [dbo].[Fact_Performance] CHECK CONSTRAINT [FK_Fact_Activities];&#xA;&#xA;    ALTER TABLE [dbo].[Fact_Performance]  WITH CHECK ADD  CONSTRAINT [FK_Fact_Parent] FOREIGN KEY([ParentalContextKey])&#xA;    REFERENCES [dbo].[Dim_ParentalContext] ([ParentalContextKey]);&#xA;&#xA;    ALTER TABLE [dbo].[Fact_Performance] CHECK CONSTRAINT [FK_Fact_Parent];&#xA;&#xA;    ALTER TABLE [dbo].[Fact_Performance]  WITH CHECK ADD  CONSTRAINT [FK_Fact_Student] FOREIGN KEY([StudentID])&#xA;    REFERENCES [dbo].[Dim_Student] ([StudentID]);&#xA;&#xA;    ALTER TABLE [dbo].[Fact_Performance] CHECK CONSTRAINT [FK_Fact_Student];&#xA;&#xA;    --------------------------------------------------&#xA;    -- 4. VALIDATION&#xA;    --------------------------------------------------&#xA;    COMMIT TRANSACTION;&#xA;    -- Pas de PRINT nécessaire pour SSIS, mais utile pour le debug manuel&#xA;    -- PRINT 'Opérations terminées avec succès.';&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    --------------------------------------------------&#xA;    -- GESTION DES ERREURS&#xA;    --------------------------------------------------&#xA;    IF @@TRANCOUNT &gt; 0&#xA;        ROLLBACK TRANSACTION;&#xA;&#xA;    -- On relève l'erreur pour que SSIS passe en rouge (Échec)&#xA;    DECLARE @ErrorMessage NVARCHAR(4000) = ERROR_MESSAGE();&#xA;    RAISERROR(@ErrorMessage, 16, 1);&#xA;END CATCH;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
